# Copy this file to .env and fill in your actual values
# DO NOT commit .env file to version control

# Google OAuth Configuration
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_REDIRECT_URI=http://localhost:5260/auth/google/callback

# Database Configuration for Local Development
DB_HOST=localhost
DB_NAME=yopo_api
DB_USERNAME=root
DB_PASSWORD=your_mysql_password
DB_PORT=3306

# Database Configuration for Production (AWS RDS)
# Uncomment and set these for production deployment
# RDS_HOSTNAME=your_production_host
# RDS_DB_NAME=your_production_db
# RDS_USERNAME=your_production_user
# RDS_PASSWORD=your_production_password
# RDS_PORT=3306

# JWT Configuration
JWT_KEY=your_super_secret_jwt_key_at_least_32_characters_long
JWT_ISSUER=yopo-api
JWT_AUDIENCE=yopo-users
